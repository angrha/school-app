<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subject</title>
  </head>
  <body>
    <table border="1">
      <tr>
        <td><strong> ID </strong></td>
        <td><strong> Subject Name </strong></td>
        <td><strong> First Name </strong></td>
        <td><strong> Last Name </strong></td>
        <td><strong> Email </strong></td>
      </tr>
      <% rowsSubject.forEach(function(list) { %>
        <tr>
          <td><%= list.id%></td>
          <td><%= list.subject_name%></td>
          <% list.Teachers.forEach(function(listTeacher){ %>
              <td><%= listTeacher.first_name%></td>
              <td><%= listTeacher.last_name%></td>
              <td><%= listTeacher.email%></td>
          <% }) %>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
